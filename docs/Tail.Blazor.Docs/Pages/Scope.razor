<PageTitle>Project Scope & Status - Tail.Blazor</PageTitle>

<div class="max-w-7xl mx-auto px-4 py-8">
    <div class="mb-8">
        <h1 class="text-4xl font-bold mb-4" style="color: var(--color-text-primary);">Project Scope & Status</h1>
        <p class="text-lg" style="color: var(--color-text-secondary);">
            Track the implementation progress and scope of the Tail.Blazor component library.
        </p>
    </div>

    <OverviewCards />

    <MissingFeaturesSection 
        HighPriorityFeatures="MissingFeaturesHighPriority"
        MediumPriorityFeatures="MissingFeaturesMediumPriority"
        LowPriorityFeatures="MissingFeaturesLowPriority" />

    <PackageDetailsSection Packages="PackageDetails" />

    <PackageStatusSection Packages="Packages" />

    <FeatureStatusSection Features="Features" />

    <RecentUpdatesSection Updates="RecentUpdates" />
</div>

